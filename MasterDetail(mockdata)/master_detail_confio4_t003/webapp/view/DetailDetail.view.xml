<mvc:View
    controllerName="com.ui5.confio4.masterdetailconfio4t003.controller.DetailDetail"
    xmlns="sap.f"
    xmlns:m="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

    <DynamicPage toggleHeaderOnTitleClick="false">
        <title>
            <DynamicPageTitle>
                <heading>
                    <m:FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
                        <m:Title text="{flight>fldate}" wrapping="true" class="sapUiTinyMarginEnd"/>
                    </m:FlexBox>
                </heading>
            </DynamicPageTitle>
        </title>
        <content>
        
            <m:Title text="{flight>OCCUPANCY}{flight>PERCENTAGE}"/>
        </content>
    </DynamicPage>
</mvc:View>